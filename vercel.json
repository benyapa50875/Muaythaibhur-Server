{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "/uploads/$1"
      }
    ],
    "env": {
      "PORT": "3000",
      "DATABASE_URL": "@database_url"
    }
  }
  